<div class="container">

	<div class="admin_photo-index mx-auto">
		<div class="row">
			<% @photos.each do |photo| %>
				<div class="col-md-3 px-0">
					<%= link_to admin_photo_path(photo.id), class: "admin_photo-index-content" do %>
						<%= attachment_image_tag photo, :image, style: 'width: 95%;', format: 'jpeg' %>
					<% end %>
				</div>
			<% end %>
		</div>
	</div>

</div>